{
    "name": "alexeevdv/yii2-zerobounce-client",
    "description": "Yii2 client for zerobounce.net API",
    "keywords": [
        "yii2",
        "zerobounce"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry V. Alekseev",
            "email": "mail@alexeevdv.ru"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "alexeevdv\\yii\\zerobounce\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "yiisoft/yii2": "~2.0.13",
        "yiisoft/yii2-httpclient": "^2.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-yii2": "^1.0.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "yiisoft/yii2-coding-standards": "^2.0"
    }
}